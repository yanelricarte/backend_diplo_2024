{{> header}}

  <h1> Productos con categoría </h1>


  <table>
    <thead>
      <tr>
        <th>ID </th>
        <th>Nombre</th>
        <th>Precio</th>
        <th>Categoría</th>
      </tr>
    </thead>
    <tbody>
      {{#each results}}
      <tr>
        <td>{{this.id}}</td>
        <td>{{this.producto_nombre}}</td>
        <td>{{this.producto_precio}}</td>
        <td>{{this.categoria_nombre}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>

  <a href="/productos"> Volver a productos</a>

  {{> footer}}

